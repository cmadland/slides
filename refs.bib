@article{scheiterTechnologyenhancedLearningTeaching2021,
  title = {Technology-Enhanced Learning and Teaching: An Overview},
  author = {Scheiter, Katharina},
  year = {2021},
  journal = {Zeitschrift f{\"u}r Erziehungswissenschaft},
  volume = {24},
