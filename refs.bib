  author = {Pham, Phuong-Tam and Duong, Tran-Binh and Phan, Thi-Thuy-Trang and Nguyen, Thai-Huu and Nguyen, Minh-Thanh and Nguyen, Manh-Tuan and Hoang, Ngoc-Anh and Nguyen, Van-Thai-Binh and Nguyen, Tien-Trung},
