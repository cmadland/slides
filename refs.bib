@article{hermannsDevelopmentUseEvaluation2022,
  title = {The {{Development}}, {{Use}}, and {{Evaluation}} of {{Digital Games}} and {{Quizzes}} in an {{Introductory Course}} on {{Organic Chemistry}} for {{Preservice Chemistry Teachers}}},
